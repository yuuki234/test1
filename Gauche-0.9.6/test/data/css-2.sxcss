(
 (style-rule body
             (padding #((2 em) (1 em) (2 em) (70 px)))
             (margin 0)
             (font-family sans-serif)
             (color black)
             (background white)
             (background-position #(top left))
             (background-attachment fixed)
             (background-repeat no-repeat))
 (style-rule (* (: link))
             (color (color "00C"))
             (background transparent))
 (style-rule (* (: visited))
             (color (color "609"))
             (background transparent))
 (style-rule (a (: active))
             (color (color "C00"))
             (background transparent))
 (style-rule (:or ((a (: link)) img) ((a (: visited)) img))
             (border-style none))
 (style-rule (a img)
             (color white))
 (|@media| (all)
           (style-rule (a img)
                       (color inherit)))
 (style-rule (:or th td)
             (font-family sans-serif))
 (style-rule (:or h1 h2 h3 h4 h5 h6)
             (text-align left))
 (style-rule (:or h1 h2 h3)
             (color (color "005A9C")) (background white))
 (style-rule h1
             (font #((170 %) sans-serif)))
 (style-rule h2
             (font #((140 %) sans-serif)))
 (style-rule h3
             (font #((120 %) sans-serif)))
 (style-rule h4
             (font #(bold (100 %) sans-serif)))
 (style-rule h5
             (font #(italic (100 %) sans-serif)))
 (style-rule h6
             (font #(small-caps (100 %) sans-serif)))
 (style-rule (* (class hide))
             (display none))
 (style-rule (div (class head))
             (margin-bottom (1 em)))
 (style-rule ((div (class head)) h1)
             (margin-top (2 em)) (clear both))
 (style-rule ((div (class head)) table)
             (margin-left (2 em))
             (margin-top (2 em)))
 (style-rule (p (class copyright))
             (font-size small))
 (style-rule ((p (class copyright)) small)
             (font-size small))
 (|@media| (screen)
           (style-rule (a (has href) (: hover))
                       (background (color "ffa"))))
 (style-rule pre
             (margin-left (2 em)))
 (style-rule (:or dt dd)
             (margin-top 0)
             (margin-bottom 0))
 (style-rule dt
             (font-weight bold))
 (style-rule (:or (ul (class toc)) (ol (class toc)))
             (list-style disc)
             (list-style none))
 (|@media| (speech)
           (style-rule (:or h1 h2 h3)
                       (voice-stress moderate))
           (style-rule (* (class hide))
                       (speak none))
           (style-rule (p (class copyright))
                       (voice-volume x-soft)
                       (voice-rate x-fast))
           (style-rule dt
                       (pause-before (63 ms))))
 (style-rule body
             (background-image (url "//www.w3.org/StyleSheets/TR/logo-REC")))
 )